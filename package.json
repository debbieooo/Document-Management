{
  "name": "docs-manager",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-doni/doc-manager.git"
  },
  "scripts": {
    "test:cover": "nyc --reporter=html --reporter=text",
    "start": "npm-run-all --parallel start:server start:client",
    "start:dev": "npm-run-all --parallel start:server:dev start:client:dev",
    "start:server:dev": "nodemon ./bin/www",
    "start:server": "node ./bin/www",
    "start:client": "node app.js",
    "start:client:dev": "webpack -w & nodemon app.js",
    "build": "webpack -p",
    "test":"NODE_ENV=test nyc --reporter=html --reporter=text mocha client/tests/helper.js client/tests/**/*.spec.js  --require server/tests/helper.js server/tests/**/*.spec.js --compilers js:babel-register", 
    "test:server":  "NODE_ENV=test nyc --reporter=html --reporter=text mocha --require server/tests/helper.js server/tests/**/*.spec.js --compilers js:babel-register",
    "test:coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:e2e": "./node_modules/.bin/nightwatch --config nightwatch.json",
    "test:client": "NODE_ENV=test nyc --reporter=html --reporter=text mocha client/tests/helper.js client/tests/**/*.spec.js --slow 5000 --compilers js:babel-register"
  },
  "nyc": {
    "exclude": [
      "server/tests",
      "client/tests"
    ],
    "extension": [
      ".jsx"
    ]
  },
  "keywords": [],
